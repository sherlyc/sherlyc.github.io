{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "commonjs",
    "noImplicitAny": true,
    "strict": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "noEmitHelpers": true,
    "target": "es5",
    "types": ["jest"],
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  }
}
