{
  "name": "stuff-experience-frontend",
  "version": "1.999.0",
  "engines": {
    "node": ">=10.15.3 <10.16",
    "npm": ">=6.4.1 <6.5"
  },
  "scripts": {
    "start": "node dist/server/serverWithNewRelic.js",
    "start:no-newrelic": "node dist/server/main.js",
    "build": "npm run build:browser && npm run build:server",
    "build:browser": "ng run stuff-experience-frontend:build:production",
    "build:server": "ng run stuff-experience-frontend:server:production && cp ./server-src/serverWithNewRelic.js ./dist/server",
    "rebuild": "npm run build && npm run start:no-newrelic",
    "test": "npm run lint && npm run test:app && npm run test:server",
    "integration-test": "npm run test:api",
    "test:server": "jest --config ./server-src/jest.config.json --coverage",
    "test:app": "jest --config ./src/jest.config.json --coverage",
    "test:api": "jest --config ./server-src/test-api/jest.config.json",
    "test:performance": "docker run -e URL_UNDER_TEST=http://host.docker.internal:4000 docker.ci.shift21.ffx.nz/nz.stuff/yokohama/yokohama-autocannon:1.3",
    "test:e2e": "concurrently --kill-others --success first npm:e2e:top-browsers npm:start:no-newrelic",
    "clinic:doctor": "clinic doctor --on-port 'autocannon localhost:$PORT' -- node ./dist/server/main.js",
    "clinic:doctor:backend": "clinic doctor --on-port 'autocannon localhost:$PORT/api/content' -- node ./dist/server/main.js",
    "clinic:flame": "clinic flame --on-port 'autocannon localhost:$PORT' -- node ./dist/server/main.js",
    "clinic:flame:backend": "clinic flame --on-port 'autocannon localhost:$PORT/api/content' -- node ./dist/server/main.js",
    "clinic:bubbleprof": "clinic bubbleprof --on-port 'autocannon -c 5 -a 500 localhost:$PORT' -- node ./dist/server/main.js",
    "clinic:bubbleprof:backend": "clinic bubbleprof --on-port 'autocannon -c 5 -a 500 localhost:$PORT/api/content' -- node ./dist/server/main.js",
    "lint": "ng lint",
    "bundle-report": "npm run build && npm run bundle-report:browser && npm run bundle-report:server",
    "bundle-report:browser": "webpack-bundle-analyzer ./dist/browser/stats.json -m static -O -r ./dist/browser.html",
    "bundle-report:server": "webpack-bundle-analyzer ./dist/server/stats.json -m static -O -r ./dist/server.html",
    "e2e:top-browsers": "concurrently --kill-others-on-fail npm:e2e:ie11 npm:e2e:firefox npm:e2e:chrome npm:e2e:safari npm:e2e:edge",
    "e2e:ie11": "E2E_BROWSER=IE11 npm run e2e",
    "e2e:chrome": "E2E_BROWSER=CHROME npm run e2e",
    "e2e:firefox": "E2E_BROWSER=FIREFOX npm run e2e",
    "e2e:safari": "E2E_BROWSER=SAFARI npm run e2e",
    "e2e:ios": "E2E_BROWSER=IOS npm run e2e",
    "e2e:android": "E2E_BROWSER=ANDROID npm run e2e",
    "e2e:edge": "E2E_BROWSER=EDGE npm run e2e",
    "e2e": "jest --config ./e2e/jest.config.json",
    "format": "prettier --write '**/*.{js,json,css,scss,md,html,yaml,ts}'",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -w -s",
    "dev": "rm -rf dist/* && concurrently -c red,magenta,cyan npm:dev:build:browser npm:dev:build:server npm:dev:serve npm:dev:proxy",
    "dev:build:browser": "ng run stuff-experience-frontend:build:development --watch",
    "dev:build:server": "wait-on dist/browser/index.html && ng run stuff-experience-frontend:server:development --watch",
    "dev:serve": "wait-on dist/server/main.js && nodemon -q --inspect=4200 -w dist/server -x 'NODE_ENV=development SPADE_ENV=localhost node dist/server/main.js'",
    "dev:proxy": "wait-on tcp:4000 && ts-node dev-proxy.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@angular/pwa": "^0.803.22",
    "@angular/router": "^8.2.14",
    "@angular/service-worker": "^8.2.14",
    "@nguniversal/express-engine": "^8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "@sentry/browser": "^5.11.0",
    "@sentry/integrations": "^5.11.0",
    "axios": "^0.19.1",
    "bowser": "^2.8.1",
    "classlist.js": "^1.1.20150312",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "custom-event-polyfill": "^1.0.7",
    "cxs": "^6.2.0",
    "default-passive-events": "^1.0.10",
    "express": "^4.17.1",
    "express-winston": "^4.0.2",
    "flexboxgrid": "^6.3.1",
    "helmet": "^3.21.2",
    "http-proxy-middleware": "^1.0.3",
    "intersection-observer": "^0.7.0",
    "lodash": "^4.17.15",
    "math-random": "^1.0.4",
    "minimatch": "^3.0.4",
    "moment": "^2.24.0",
    "newrelic": "^6.2.0",
    "p-retry": "^4.2.0",
    "polyfill-array-includes": "^2.0.0",
    "pulltorefreshjs": "^0.1.20",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^6.5.4",
    "source-map-support": "^0.5.16",
    "store": "^2.0.12",
    "uuid": "^3.3.3",
    "web-animations-js": "^2.3.2",
    "winston": "^3.2.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.4.1",
    "@angular-builders/jest": "^8.3.2",
    "@angular-devkit/build-angular": "^0.803.22",
    "@angular/cli": "^8.3.22",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@compodoc/compodoc": "^1.1.11",
    "@types/cookie": "^0.3.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/express-winston": "^4.0.0",
    "@types/helmet": "^0.0.45",
    "@types/http-proxy": "^1.17.3",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/jest": "^24.9.0",
    "@types/lodash": "^4.14.149",
    "@types/minimatch": "^3.0.3",
    "@types/moment": "^2.13.0",
    "@types/newrelic": "^5.11.2",
    "@types/node": "^12.12.24",
    "@types/selenium-webdriver": "^4.0.6",
    "@types/source-map-support": "^0.5.0",
    "@types/store": "^2.0.2",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.6",
    "@types/webpack": "^4.41.2",
    "@types/winston": "^2.4.4",
    "bourbon": "^6.0.0",
    "browserstack-local": "^1.4.3",
    "chromedriver": "^78.0.1",
    "clinic": "^5.0.0",
    "codelyzer": "^5.2.1",
    "http-proxy": "^1.18.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-preset-angular": "^7.1.1",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "normalize.css": "^8.0.1",
    "prettier": "^1.19.1",
    "rxjs-tslint": "^0.1.7",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tslib": "^1.10.0",
    "tslint": "^5.20.1",
    "typescript": "3.5.3",
    "utility-types": "^3.10.0",
    "wait-on": "^3.3.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10"
  },
  "optionalDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint && npm run test:app && npm run test:server"
    }
  },
  "lint-staged": {
    "**/*.{js,json,css,scss,md,html,yaml,ts}": [
      "prettier --write",
      "git add"
    ]
  }
}
