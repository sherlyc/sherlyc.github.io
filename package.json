{
  "name": "stuff-experience-frontend",
  "version": "1.999.0",
  "engines": {
    "node": ">=10.15.3 <10.16",
    "npm": ">=6.4.1 <6.5"
  },
  "scripts": {
    "start": "node dist/server/serverWithNewRelic.js",
    "pre-build": "ngcc",
    "start:no-newrelic": "node dist/server/main.js",
    "build": "npm run build:browser && npm run build:server",
    "build:browser": "ng run stuff-experience-frontend:build:production",
    "build:server": "ng run stuff-experience-frontend:server:production && cp ./server-src/serverWithNewRelic.js ./dist/server",
    "rebuild": "npm run build && npm run start:no-newrelic",
    "test": "npm run lint && npm run test:app && npm run test:server",
    "integration-test": "npm run test:api",
    "test:server": "jest --config ./server-src/jest.config.json --coverage",
    "test:app": "jest --config ./src/jest.config.json --coverage",
    "test:api": "jest --config ./server-src/test-api/jest.config.json",
    "test:performance": "docker run -e URL_UNDER_TEST=http://host.docker.internal:4000 docker.ci.shift21.ffx.nz/nz.stuff/yokohama/yokohama-autocannon:1.3",
    "test:e2e": "concurrently --kill-others --success first npm:e2e:bs-browsers npm:start:no-newrelic",
    "clinic:doctor": "clinic doctor --on-port 'autocannon localhost:$PORT' -- node ./dist/server/main.js",
    "clinic:doctor:backend": "clinic doctor --on-port 'autocannon localhost:$PORT/api/content' -- node ./dist/server/main.js",
    "clinic:flame": "clinic flame --on-port 'autocannon localhost:$PORT' -- node ./dist/server/main.js",
    "clinic:flame:backend": "clinic flame --on-port 'autocannon localhost:$PORT/api/content' -- node ./dist/server/main.js",
    "clinic:bubbleprof": "clinic bubbleprof --on-port 'autocannon -c 5 -a 500 localhost:$PORT' -- node ./dist/server/main.js",
    "clinic:bubbleprof:backend": "clinic bubbleprof --on-port 'autocannon -c 5 -a 500 localhost:$PORT/api/content' -- node ./dist/server/main.js",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix=true",
    "bundle-report": "npm run build && npm run bundle-report:browser && npm run bundle-report:server",
    "bundle-report:browser": "webpack-bundle-analyzer ./dist/browser/stats.json -m static -O -r ./dist/browser.html",
    "bundle-report:server": "webpack-bundle-analyzer ./dist/server/stats.json -m static -O -r ./dist/server.html",
    "format": "prettier --write '**/*.{js,json,css,scss,md,html,yaml,ts}'",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.app.json -w -s",
    "dev": "rm -rf dist/* && concurrently -c red,magenta,cyan npm:dev:build:browser npm:dev:build:server npm:dev:serve npm:dev:proxy",
    "dev:build:browser": "ng run stuff-experience-frontend:build:development --watch",
    "dev:build:server": "wait-on dist/browser/index.html && ng run stuff-experience-frontend:server:development --watch",
    "dev:serve": "wait-on dist/server/main.js && nodemon -q --inspect=4200 -w dist/server -x 'NODE_ENV=development SPADE_ENV=localhost node dist/server/main.js'",
    "dev:proxy": "wait-on tcp:4000 && ts-node --project tsconfig.server.json dev-proxy.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/common": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/platform-server": "^9.1.9",
    "@angular/router": "^9.1.9",
    "@angular/service-worker": "^9.1.9",
    "@nguniversal/express-engine": "^9.1.1",
    "@sentry/browser": "^5.20.1",
    "@sentry/integrations": "^5.20.1",
    "axios": "^0.19.2",
    "bowser": "^2.9.0",
    "classlist.js": "^1.1.20150312",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "custom-event-polyfill": "^1.0.7",
    "cxs": "^6.2.0",
    "date-fns": "^2.14.0",
    "date-fns-tz": "^1.0.10",
    "default-passive-events": "^1.0.10",
    "express": "^4.17.1",
    "express-winston": "^4.0.3",
    "flexboxgrid": "^6.3.1",
    "helmet": "^3.22.0",
    "http-proxy-middleware": "^1.0.4",
    "intersection-observer": "^0.10.0",
    "lodash-es": "^4.17.15",
    "math-random": "^1.0.4",
    "minimatch": "^3.0.4",
    "newrelic": "^6.8.0",
    "p-retry": "^4.2.0",
    "polyfill-array-includes": "^2.0.0",
    "pulltorefreshjs": "^0.1.20",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^6.5.5",
    "source-map-support": "^0.5.19",
    "uuid": "^8.1.0",
    "web-animations-js": "^2.3.2",
    "winston": "^3.2.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.1.0",
    "@angular-builders/jest": "^9.0.1",
    "@angular-devkit/build-angular": "^0.901.7",
    "@angular-devkit/schematics": "^9.1.7",
    "@angular/cli": "^9.1.7",
    "@angular/compiler": "^9.1.9",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/language-service": "^9.1.9",
    "@compodoc/compodoc": "^1.1.11",
    "@schematics/angular": "^9.1.7",
    "@types/cookie": "^0.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/doubleclick-gpt": "^2019041801.0.2",
    "@types/express": "^4.17.6",
    "@types/express-winston": "^4.0.0",
    "@types/helmet": "^0.0.47",
    "@types/http-proxy": "^1.17.4",
    "@types/jasminewd2": "^2.0.8",
    "@types/jest": "^25.2.3",
    "@types/lodash-es": "^4.17.3",
    "@types/math-random": "^1.0.0",
    "@types/minimatch": "^3.0.3",
    "@types/newrelic": "^6.4.0",
    "@types/node": "^14.0.11",
    "@types/source-map-support": "^0.5.1",
    "@types/supertest": "^2.0.9",
    "@types/uuid": "^8.0.0",
    "@types/webpack": "^4.41.17",
    "@types/winston": "^2.4.4",
    "bourbon": "^7.0.0",
    "chromedriver": "^83.0.0",
    "clinic": "^6.0.0",
    "codelyzer": "^5.2.2",
    "http-proxy": "^1.18.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-preset-angular": "^8.2.0",
    "lint-staged": "^10.2.9",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "prettier": "^2.0.5",
    "rxjs-tslint": "^0.1.8",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tslib": "^2.0.0",
    "tslint": "^6.1.2",
    "typescript": "3.8.3",
    "utility-types": "^3.10.0",
    "wait-on": "^5.0.1",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11"
  },
  "optionalDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint-fix && npm run test:app && npm run test:server"
    }
  },
  "lint-staged": {
    "**/*.{js,json,css,scss,md,html,yaml,ts}": [
      "prettier --write",
      "git add"
    ]
  }
}
