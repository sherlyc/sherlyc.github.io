{
  "name": "stuff-experience-frontend",
  "version": "1.999.0",
  "scripts": {
    "start": "node dist/server/serverWithNewRelic.js",
    "start:no-newrelic": "node dist/server/main.js",
    "build": "npm run build:browser && npm run build:server",
    "build:browser": "ng run stuff-experience-frontend:build:production --stats-json",
    "build:server": "ng run stuff-experience-frontend:server:production --stats-json && cp ./server-src/serverWithNewRelic.js ./dist/server",
    "test": "npm run lint && npm run test:app && npm run test:server",
    "integration-test": "npm run test:api",
    "test:server": "jest --config ./server-src/jest.config.json --coverage",
    "test:app": "jest --config ./src/jest.config.json --coverage",
    "test:api": "jest --config ./server-src/test-api/jest.config.json",
    "test:performance": "docker run -e URL_UNDER_TEST=http://host.docker.internal:4000 docker.ci.shift21.ffx.nz/nz.stuff/yokohama/yokohama-autocannon:1.3",
    "test:e2e": "concurrently --kill-others --success first npm:e2e:top-browsers npm:start:no-newrelic",
    "clinic:doctor": "clinic doctor --on-port 'autocannon localhost:$PORT' -- node ./dist/server/main.js",
    "clinic:doctor:backend": "clinic doctor --on-port 'autocannon localhost:$PORT/api/content' -- node ./dist/server/main.js",
    "clinic:flame": "clinic flame --on-port 'autocannon localhost:$PORT' -- node ./dist/server/main.js",
    "clinic:flame:backend": "clinic flame --on-port 'autocannon localhost:$PORT/api/content' -- node ./dist/server/main.js",
    "clinic:bubbleprof": "clinic bubbleprof --on-port 'autocannon -c 5 -a 500 localhost:$PORT' -- node ./dist/server/main.js",
    "clinic:bubbleprof:backend": "clinic bubbleprof --on-port 'autocannon -c 5 -a 500 localhost:$PORT/api/content' -- node ./dist/server/main.js",
    "lint": "ng lint",
    "bundle-report": "npm run build && npm run bundle-report:browser && npm run bundle-report:server",
    "bundle-report:browser": "webpack-bundle-analyzer ./dist/browser/stats.json -m 'static' -O -r './dist/browser.html'",
    "bundle-report:server": "webpack-bundle-analyzer ./dist/server/stats.json -m 'static' -O -r './dist/server.html'",
    "e2e:top-browsers": "concurrently --kill-others-on-fail npm:e2e:ie11 npm:e2e:firefox npm:e2e:chrome npm:e2e:android npm:e2e:safari npm:e2e:edge",
    "e2e:ie11": "E2E_BROWSER=IE11 npm run e2e",
    "e2e:chrome": "E2E_BROWSER=CHROME npm run e2e",
    "e2e:firefox": "E2E_BROWSER=FIREFOX npm run e2e",
    "e2e:safari": "E2E_BROWSER=SAFARI npm run e2e",
    "e2e:ios": "E2E_BROWSER=IOS npm run e2e",
    "e2e:android": "E2E_BROWSER=ANDROID npm run e2e",
    "e2e:edge": "E2E_BROWSER=EDGE npm run e2e",
    "e2e": "jest --config ./e2e/jest.config.json",
    "format": "prettier --write '**/*.{js,json,css,scss,md,html,yaml,ts}'",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -w -s",
    "dev": "concurrently -c red,green,magenta,cyan,blue npm:dev:build:browser npm:dev:build:server npm:dev:serve npm:dev:proxy",
    "dev:build": "ng run stuff-experience-frontend:build && ng run stuff-experience-frontend:server",
    "dev:build:browser": "ng run stuff-experience-frontend:build --watch",
    "dev:build:server": "ng run stuff-experience-frontend:server --watch",
    "dev:serve": "NODE_ENV=development SPADE_ENV=development nodemon --delay 2.5 --inspect=4200 --watch dist dist/server/main.js",
    "dev:proxy": "ts-node dev-proxy.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/common": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/platform-server": "^7.2.15",
    "@angular/pwa": "^0.13.9",
    "@angular/router": "^7.2.15",
    "@angular/service-worker": "^7.2.15",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "@sentry/browser": "^5.2.1",
    "@types/jest": "^24.0.15",
    "@types/store": "^2.0.1",
    "axios": "^0.18.0",
    "browserstack-local": "^1.4.0",
    "classlist.js": "^1.1.20150312",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.4",
    "core-js": "^2.6.5",
    "cors": "^2.8.5",
    "default-passive-events": "^1.0.10",
    "flexboxgrid": "^6.3.1",
    "helmet": "^3.18.0",
    "intersection-observer": "^0.5.1",
    "lodash": "^4.17.11",
    "math-random": "^1.0.4",
    "minimatch": "^3.0.4",
    "moment": "^2.24.0",
    "newrelic": "^5.8.0",
    "p-retry": "^4.1.0",
    "polyfill-array-includes": "^2.0.0",
    "rxjs": "^6.5.2",
    "source-map-support": "^0.5.12",
    "store": "^2.0.12",
    "uuid": "^3.3.2",
    "web-animations-js": "^2.3.1",
    "winston": "^3.2.1",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^7.4.3",
    "@angular-builders/jest": "^8.0.3",
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "~7.3.9",
    "@angular/compiler": "^7.2.15",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@compodoc/compodoc": "^1.1.9",
    "@types/cookie": "^0.3.2",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.16.1",
    "@types/helmet": "0.0.43",
    "@types/http-proxy": "^1.17.0",
    "@types/jasmine": "^3.3.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.126",
    "@types/minimatch": "^3.0.3",
    "@types/moment": "^2.13.0",
    "@types/newrelic": "^4.11.0",
    "@types/node": "^12.0.1",
    "@types/selenium-webdriver": "^4.0.0",
    "@types/source-map-support": "^0.5.0",
    "@types/supertest": "^2.0.7",
    "@types/uuid": "^3.4.4",
    "@types/webpack": "^4.4.31",
    "@types/winston": "^2.4.4",
    "bourbon": "4.2.7",
    "chromedriver": "^74.0.0",
    "clinic": "^4.0.3",
    "codelyzer": "^4.5.0",
    "http-proxy": "^1.17.0",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "jest-preset-angular": "^7.1.1",
    "lint-staged": "^8.1.6",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "prettier": "1.17.1",
    "rxjs-tslint": "^0.1.7",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "supertest": "^3.4.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.0",
    "ts-node": "~8.1.0",
    "tslib": "^1.9.3",
    "tslint": "^5.16.0",
    "typescript": "3.2.4",
    "utility-types": "^3.7.0",
    "webpack": "^4.31.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2"
  },
  "optionalDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint && npm run test:app && npm run test:server"
    }
  },
  "lint-staged": {
    "**/*.{js,json,css,scss,md,html,yaml,ts}": [
      "prettier --write",
      "git add"
    ]
  }
}
