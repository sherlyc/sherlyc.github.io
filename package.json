{
  "name": "stuff-experience-frontend",
  "version": "0.999.0",
  "scripts": {
    "start": "node dist/server/main.js",
    "build": "npm run build:browser && npm run build:server",
    "build:browser": "ng run stuff-experience-frontend:build:production --stats-json",
    "build:server": "ng run stuff-experience-frontend:server:production --stats-json",
    "test": "npm run test:app && npm run test:server && npm run test:api",
    "test:server": "jest --config ./server-src/jest.config.json --coverage",
    "test:app": "jest --config ./src/jest.config.json --coverage",
    "test:api": "jest --config ./server-src/test-api/jest.config.json",
    "lint": "ng lint",
    "bundle-report": "npm run build && npm run bundle-report:browser && npm run bundle-report:server",
    "bundle-report:browser": "webpack-bundle-analyzer ./dist/browser/stats.json -m 'static' -O -r './dist/browser.html'",
    "bundle-report:server": "webpack-bundle-analyzer ./dist/server/stats.json -m 'static' -O -r './dist/server.html'",
    "e2e": "jest --config ./e2e/jest.config.json",
    "format": "prettier --write '**/*.{js,json,css,scss,md,html,yaml,ts}'",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -w -s",
    "dev": "concurrently -c red,green,magenta,cyan,blue npm:dev:build:browser npm:dev:build:server npm:dev:serve",
    "dev:build": "ng run stuff-experience-frontend:build && ng run stuff-experience-frontend:server",
    "dev:build:browser": "ng run stuff-experience-frontend:build --watch",
    "dev:build:server": "ng run stuff-experience-frontend:server --watch",
    "dev:serve": "NODE_ENV=development SPADE_ENV=development nodemon --delay 2.5 --watch dist dist/server/main.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.8",
    "@angular/common": "^7.2.8",
    "@angular/core": "^7.2.8",
    "@angular/http": "^7.2.8",
    "@angular/platform-browser": "^7.2.8",
    "@angular/platform-browser-dynamic": "^7.2.8",
    "@angular/platform-server": "^7.2.8",
    "@angular/pwa": "^0.13.5",
    "@angular/router": "^7.2.8",
    "@angular/service-worker": "^7.2.8",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "@sentry/browser": "^4.6.4",
    "axios": "^0.18.0",
    "classlist.js": "^1.1.20150312",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.4",
    "core-js": "^2.6.5",
    "cors": "^2.8.5",
    "flexboxgrid": "^6.3.1",
    "helmet": "^3.16.0",
    "intersection-observer": "^0.5.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "ngx-moment": "^3.4.0",
    "p-retry": "^3.0.1",
    "rxjs": "^6.4.0",
    "source-map-support": "^0.5.10",
    "web-animations-js": "^2.3.1",
    "winston": "^3.2.1",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^7.4.3",
    "@angular-builders/jest": "^7.3.1",
    "@angular-devkit/build-angular": "^0.12.4",
    "@angular/cli": "~7.0.6",
    "@angular/compiler": "^7.2.8",
    "@angular/compiler-cli": "^7.2.8",
    "@angular/language-service": "^7.2.8",
    "@compodoc/compodoc": "^1.1.9",
    "@types/cookie": "^0.3.2",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/helmet": "0.0.42",
    "@types/http-proxy": "^1.17.0",
    "@types/jasmine": "^3.3.9",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.123",
    "@types/minimatch": "^3.0.3",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.12.30",
    "@types/p-retry": "^3.0.0",
    "@types/selenium-webdriver": "^3.0.15",
    "@types/source-map-support": "^0.4.2",
    "@types/supertest": "^2.0.7",
    "@types/webpack": "^4.4.25",
    "@types/winston": "^2.4.4",
    "bourbon": "4.2.7",
    "chromedriver": "^2.46.0",
    "codelyzer": "^4.5.0",
    "http-proxy": "^1.17.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.2",
    "lint-staged": "^8.1.5",
    "minimatch": "^3.0.4",
    "node-sass": "^4.11.0",
    "normalize.css": "^8.0.1",
    "prettier": "1.15.3",
    "rxjs-tslint": "^0.1.7",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "supertest": "^3.4.2",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "ts-node": "~7.0.0",
    "tslib": "^1.9.3",
    "tslint": "^5.13.1",
    "typescript": "~3.1.6",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.2.3"
  },
  "optionalDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint && npm run test:app && npm run test:server"
    }
  },
  "lint-staged": {
    "**/*.{js,json,css,scss,md,html,yaml,ts}": [
      "prettier --write",
      "git add"
    ]
  }
}
